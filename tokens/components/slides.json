{
  "image_border-radius": {
    "value": "{theme_border-radius.default}",
    "type": "borderRadius"
  },
  "slides_parent": {
    "value": {
      "width": "{theme_dimensions.slides-width}",
      "height": "{theme_dimensions.slides-height}",
      "fill": "{theme_colors.slide-bg.default}",
      "borderRadius": "{theme_border-radius.default}"
    },
    "type": "composition"
  },
  "slide_border-radius": {
    "value": "{theme_border-radius.default}",
    "type": "borderRadius"
  },
  "slides_type": {
    "title-emphasis": {
      "value": "{theme_typography.title-large}",
      "type": "typography"
    },
    "title-big": {
      "value": "{theme_typography.title-default}",
      "type": "typography"
    },
    "title-small": {
      "value": "{theme_typography.title-small}",
      "type": "typography"
    },
    "subtext-bold": {
      "value": "{theme_typography.title-small}",
      "type": "typography"
    },
    "subtext": {
      "value": "{theme_typography.long-text}",
      "type": "typography"
    }
  },
  "slides_border": {
    "default": {
      "value": "{theme_border.slides}",
      "type": "border"
    }
  },
  "slides_colors": {
    "text": {
      "subtext": {
        "value": "{theme_colors.slide-fg.default}",
        "type": "color"
      },
      "titles": {
        "value": "{theme_colors.slide-fg.muted}",
        "type": "color"
      }
    },
    "fg": {
      "lines": {
        "value": "{theme_colors.slide-fg.accent}",
        "type": "color"
      }
    },
    "bg": {
      "default": {
        "value": "{theme_colors.slide-bg.default}",
        "type": "color"
      }
    }
  }
}